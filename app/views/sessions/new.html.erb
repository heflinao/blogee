<h1>Sign In</h1>
	<%= form_for :signin, method: "POST" do |f| %>
		<p>
			<%= f.label :username %>
			<%= f.text_field :username %>
		</p>
		<p>
			<%= f.label :email %>
			<%= f.text_field :email %>
		</p>
		<p>
			<%= f.label :password %>
			<%= f.password_field :password %>
		</p>
		<%= f.submit "Sign in" %>
<% end %>
